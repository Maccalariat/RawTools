# RawToolsTest
cmake_minimum_required(VERSION 3.7 FATAL_ERROR)
project(rawToolsTest LANGUAGES CXX)
set_target_properties(rawToolsTest PROPERTIES
  CXX_STANDARD 14
  CXX_STANDARD_REQUIRED YES
  CXX_EXTENSIONS NO
  ARCHIVE_OUTPUT_DIRECTORY "${CMAKE_BINARY_DIR}/${CMAKE_BUILD_TYPE}/arc"
  LIBRARY_OUTPUT_DIRECTORY "${CMAKE_BINARY_DIR}/${CMAKE_BUILD_TYPE}/lib"
  RUNTIME_OUTPUT_DIRECTORY "${CMAKE_BINARY_DIR}/${CMAKE_BUILD_TYPE}/bin"
  )
target_sources(rawToolsTest
  PUBLIC
  "rawToolstest/rawToolsTest.cpp")

add_executable(rawToolsTest)
